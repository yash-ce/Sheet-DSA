// https://practice.geeksforgeeks.org/problems/rotate-array-by-n-elements
#include <iostream>
using namespace std;

int main() {
	//code
	long int t;
	cin>>t;
	while(t--){
	    long int n, d;
	    cin>>n>>d;
	    
	    long int a[n];
	    for(long int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    
	    for(long int i=d;i<n;i++){
	        cout<<a[i]<<" ";
	    }
	    for(long int i=0;i<d;i++){
	        cout<<a[i]<<" ";
	    }
	    
	    cout<<endl;
	    
	}
	return 0;
}
