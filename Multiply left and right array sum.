//https://practice.geeksforgeeks.org/problems/multiply-left-and-right-array-sum1555/1

//User function Template for C++

int multiply(int arr[], int n)
{
    // Complete the function
    int mid = n/2;
    int left = 0;
    int right = 0;
    for(int i=0;i<mid;i++){
        left += arr[i];
    }
     for(int i=mid;i<n;i++){
        right += arr[i];
    }
    return left*right;
    
}
